<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="570.0" prefWidth="400.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="album.Controller.DroiteInventairePhotos">
   <children>
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="20.0" prefWidth="300.0" text="Add Photos" textAlignment="CENTER" textFill="#0e0ed7" onAction="#AddPhotoClick">
         <VBox.margin>
            <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font name="Bebas Neue" size="20.0" />
         </font>
      </Button>
      <ScrollPane prefHeight="490.0" prefWidth="337.0" fitToHeight="true" fitToWidth="true" >
         <content>
            <GridPane prefHeight="0" prefWidth="300.0" fx:id="inventaire_photos">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
            </GridPane>
         </content>
      </ScrollPane>
   </children>
</VBox>
